{
  "name": "credo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "release:desktop": "node scripts/release-desktop"
  },
  "devDependencies": {
    "prettier": "latest"
  },
  "engines": {
    "node": ">=14.0.0 <16.20.0"
  },
  "packageManager": "pnpm@7.27.0",
  "dependencies": {
    "minimist": "^1.2.7",
    "semver": "^7.3.8"
  }
}